   <div class="card">
      <div class="card-header d-flex justify-content-between">
         <div class="header-title">
            <h4 class="card-title">Filter</h4>
         </div>
      </div>
      <div class="card-body">
        <form>
          <!-- Role Filter -->
          <div class="form-group">
            <label class="form-label">Internship Role:</label>
            <select name="role" class="selectpicker form-control" data-style="py-0">
              <option>Select Role</option>
              <option>Web Designer</option>
              <option>Web Developer</option>
              <option>Tester</option>
              <option>PHP Developer</option>
              <option>iOS Developer</option>
              <option>Android Developer</option>
              <option>Marketing Intern</option>
            </select>
          </div>

          <!-- Location Filter -->
          <div class="form-group">
            <label class="form-label">Location:</label>
            <select name="location" class="selectpicker form-control" data-style="py-0">
              <option>Select Location</option>
              <option>Mumbai</option>
              <option>Bangalore</option>
              <option>Delhi</option>
              <option>Pune</option>
              <option>Chennai</option>
            </select>
          </div>

          <!-- Salary Range Slider -->
          <div class="form-group">
            <label class="form-label">Salary Range (per month):</label>
            <input type="range" class="form-control-range" id="salaryRange" min="1000" max="50000" step="1000" value="10000">
            <div class="d-flex justify-content-between">
              <span>₹1,000</span>
              <span>₹50,000</span>
            </div>
            <input type="text" id="salaryDisplay" class="form-control" value="₹10,000" readonly>
          </div>

          <!-- Duration Filter -->
          <div class="form-group">
            <label class="form-label">Internship Duration:</label>
            <select name="duration" class="selectpicker form-control" data-style="py-0">
              <option>Select Duration</option>
              <option>1 month</option>
              <option>2 months</option>
              <option>3 months</option>
              <option>6 months</option>
            </select>
          </div>

          <!-- Apply Button -->
          <div class="form-group mb-0">
            <input type="submit" class="btn-primary btn" value="Apply">
          </div>
        </form>
      </div>
 
 



   </div>
