<%- include('./includes/studentHeader') %>
   <div class="iq-navbar-header" style="height: 215px;">
      <div class="container-fluid iq-container">
         <div class="row">
            <div class="col-md-12">
               <div class="flex-wrap d-flex justify-content-between align-items-center">
                  <div>
                     <h1>Shail Set This page up</h1>
                     <!-- <p>We are on a mission to help developers like you build successful projects for FREE.</p> -->
                  </div>
               </div>
            </div>
         </div>
      </div>
      <div class="iq-header-img">
         <img src="../../assets/images/dashboard/top-header.png" alt="header"
            class="theme-color-default-img img-fluid w-100 h-100 animated-scaleX">
         <img src="../../assets/images/dashboard/top-header1.png" alt="header"
            class="theme-color-purple-img img-fluid w-100 h-100 animated-scaleX">
         <img src="../../assets/images/dashboard/top-header2.png" alt="header"
            class="theme-color-blue-img img-fluid w-100 h-100 animated-scaleX">
         <img src="../../assets/images/dashboard/top-header3.png" alt="header"
            class="theme-color-green-img img-fluid w-100 h-100 animated-scaleX">
         <img src="../../assets/images/dashboard/top-header4.png" alt="header"
            class="theme-color-yellow-img img-fluid w-100 h-100 animated-scaleX">
         <img src="../../assets/images/dashboard/top-header5.png" alt="header"
            class="theme-color-pink-img img-fluid w-100 h-100 animated-scaleX">
      </div>
   </div> <!-- Nav Header Component End -->
   <!--Nav End-->

   <div class="container-fluid py-4">
      <div class="row">
         <div class="col-lg-12">
            <div class="container py-5">
               <!-- Header Section -->
               <div class="card bg-primary mb-4">
                  <div class="card-body p-4">
                     <div class="row align-items-center">
                        <div class="col-md-2 text-center">
                           <img src="<%= internshipDoc.companyLogo %>" alt="Company Logo"
                              class="img-fluid rounded-3 mb-3" style="max-width: 180px;">
                        </div>
                        <div class="col-md-10 text-white">
                           <h1 class="display-4 mb-2 fw-bold">
                              <%= internshipDoc.jobTitle %>
                           </h1>
                           <h3 class="h3 mb-3">
                              <%= internshipDoc.companyName %>
                           </h3>
                           <div class="d-flex flex-wrap gap-2">
                              <span class="badge bg-light text-dark"><i class="fas fa-map-marker-alt me-2"></i>New York,
                                 USA</span>
                              <span class="badge bg-success"><i class="fas fa-briefcase me-2"></i>Full-Time</span>
                              <span class="badge bg-info"><i class="fas fa-laptop-house me-2"></i>Remote Work
                                 Available</span>
                           </div>
                        </div>
                     </div>
                  </div>
               </div>

               <!-- Quick Info Card -->
               <div class="card shadow-sm mb-4">
                  <div class="card-header bg-soft-primary">
                     <h5 class="mb-0"><i class="fas fa-info-circle me-2"></i>Key Details</h5>
                  </div>
                  <div class="card-body">
                     <div class="row">
                        <div class="col-md-6">
                           <div class="p-3 bg-soft-primary rounded mb-3">
                              <i class="fas fa-money-bill-wave me-2"></i>
                              <strong>Salary Range:</strong> $80,000 - $100,000
                           </div>
                           <div class="p-3 bg-soft-primary rounded mb-3">
                              <i class="fas fa-calendar-alt me-2"></i>
                              <strong>Posted Date:</strong> 2024-11-18
                           </div>
                           <div class="p-3 bg-soft-primary rounded">
                              <i class="fas fa-clock me-2"></i>
                              <strong>Work Schedule:</strong> 9 AM - 5 PM
                           </div>
                        </div>
                        <div class="col-md-6">
                           <div class="p-3 bg-soft-primary rounded mb-3">
                              <i class="fas fa-graduation-cap me-2"></i>
                              <strong>Education:</strong> Bachelor's Degree
                           </div>
                           <div class="p-3 bg-soft-primary rounded mb-3">
                              <i class="fas fa-briefcase me-2"></i>
                              <strong>Experience:</strong> 2+ years
                           </div>
                           <div class="p-3 bg-soft-primary rounded">
                              <i class="fas fa-plane me-2"></i>
                              <strong>Travel:</strong> Occasional
                           </div>
                        </div>
                     </div>
                  </div>
               </div>

               <!-- Job Description -->
               <div class="card shadow-sm mb-4">
                  <div class="card-header bg-soft-primary">
                     <h5 class="mb-0"><i class="fas fa-align-left me-2"></i>Job Description</h5>
                  </div>
                  <div class="card-body">
                     <p class="lead">We are looking for a skilled Software Engineer to join our team. You will be
                        responsible for designing, developing, and maintaining scalable applications.</p>
                  </div>
               </div>

               <!-- Responsibilities & Qualifications -->
               <div class="row">
                  <div class="col-md-6">
                     <div class="card shadow-sm mb-4">
                        <div class="card-header bg-soft-primary">
                           <h5 class="mb-0"><i class="fas fa-tasks me-2"></i>Responsibilities</h5>
                        </div>
                        <div class="card-body">
                           <ul class="list-group list-group-flush">
                              <li class="list-group-item"><i class="fas fa-check-circle text-success me-2"></i>Develop
                                 and maintain software solutions</li>
                              <li class="list-group-item"><i
                                    class="fas fa-check-circle text-success me-2"></i>Collaborate with cross-functional
                                 teams</li>
                              <li class="list-group-item"><i class="fas fa-check-circle text-success me-2"></i>Debug and
                                 optimize code</li>
                           </ul>
                        </div>
                     </div>
                  </div>
                  <div class="col-md-6">
                     <div class="card shadow-sm mb-4">
                        <div class="card-header bg-soft-primary">
                           <h5 class="mb-0"><i class="fas fa-clipboard-check me-2"></i>Qualifications</h5>
                        </div>
                        <div class="card-body">
                           <ul class="list-group list-group-flush">
                              <li class="list-group-item"><i class="fas fa-check-circle text-success me-2"></i>Strong
                                 knowledge of JavaScript</li>
                              <li class="list-group-item"><i
                                    class="fas fa-check-circle text-success me-2"></i>Experience with React.js and
                                 Node.js</li>
                              <li class="list-group-item"><i class="fas fa-check-circle text-success me-2"></i>Excellent
                                 problem-solving skills</li>
                           </ul>
                        </div>
                     </div>
                  </div>
               </div>

               <!-- Skills & Languages -->
               <div class="row">
                  <div class="col-md-6">
                     <div class="card shadow-sm mb-4">
                        <div class="card-header bg-soft-primary">
                           <h5 class="mb-0"><i class="fas fa-tools me-2"></i>Required Skills</h5>
                        </div>
                        <div class="card-body">
                           <div class="d-flex flex-wrap gap-2">
                              <span class="badge bg-secondary">JavaScript</span>
                              <span class="badge bg-secondary">React</span>
                              <span class="badge bg-secondary">Node.js</span>
                           </div>
                        </div>
                     </div>
                  </div>
                  <div class="col-md-6">
                     <div class="card shadow-sm mb-4">
                        <div class="card-header bg-soft-primary">
                           <h5 class="mb-0"><i class="fas fa-language me-2"></i>Required Languages</h5>
                        </div>
                        <div class="card-body">
                           <div class="d-flex flex-wrap gap-2">
                              <span class="badge bg-secondary">English</span>
                              <span class="badge bg-secondary">Spanish</span>
                           </div>
                        </div>
                     </div>
                  </div>
               </div>

               <!-- Benefits -->
               <div class="card shadow-sm mb-4">
                  <div class="card-header bg-soft-primary">
                     <h5 class="mb-0"><i class="fas fa-gift me-2"></i>Benefits</h5>
                  </div>
                  <div class="card-body">
                     <div class="row">
                        <div class="col-md-4">
                           <div class="p-3 bg-soft-primary rounded mb-3">
                              <i class="fas fa-check-circle text-success me-2"></i>Health Insurance
                           </div>
                        </div>
                        <div class="col-md-4">
                           <div class="p-3 bg-soft-primary rounded mb-3">
                              <i class="fas fa-check-circle text-success me-2"></i>Paid Leave
                           </div>
                        </div>
                        <div class="col-md-4">
                           <div class="p-3 bg-soft-primary rounded mb-3">
                              <i class="fas fa-check-circle text-success me-2"></i>Retirement Plan
                           </div>
                        </div>
                     </div>
                  </div>
               </div>

               <!-- Application Info -->
               <div class="card shadow-sm mb-4">
                  <div class="card-header bg-soft-primary">
                     <h5 class="mb-0"><i class="fas fa-file-alt me-2"></i>Application Information</h5>
                  </div>
                  <div class="card-body">
                     <div class="bg-soft-primary rounded p-4">
                        <p class="mb-3"><i class="fas fa-user me-2"></i><strong>Contact Person:</strong> John Doe</p>
                        <p class="mb-3"><i class="fas fa-envelope me-2"></i><strong>Email:</strong> john.doe@example.com
                        </p>
                        <p class="mb-3"><i class="fas fa-phone me-2"></i><strong>Phone:</strong> +123 456 7890</p>
                        <button class="btn btn-primary btn-lg">
                           <i class="fas fa-paper-plane me-2"></i>Apply Now
                        </button>
                     </div>
                  </div>
               </div>
            </div>
         </div>
      </div>
   </div>

   <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0/dist/js/bootstrap.bundle.min.js"></script>
   <!-- Hope UI JS -->
   <script src="https://cdn.jsdelivr.net/npm/hope-ui@0.2.2/dist/hope-ui.min.js"></script>

   <%- include('./includes/studentFooter') %>